#include <iostream>
#include <string>

#include "fbb-git.gitlab.io_-_PersonClass.hpp"

using namespace std;

int main(int argc, char *argv[])
{
    Person person;

	person.setName("Linus Torvalds");
	person.setAddress("E-mail: Torvalds@cs.helsinki.fi");
	person.setPhone("");
	person.setMass(75);           // kg.
	
	cout << person << std::endl;
	
	return 1;
}
