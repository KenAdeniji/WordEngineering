<UserControl x:Class="SilverlightApplicationResume.CareerSummary"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">

    <StackPanel Orientation="Vertical">
        <TextBlock Margin="0, 8"><Bold>CAREER SUMMARY</Bold></TextBlock>
        <TextBlock Text="19-year track record in mission-critical roles." Margin="24,0,0,0"/>
        <TextBlock Text="Domain knowledge in the banking finance, electronic commerce, manufacturing, entertainment, health, communication, on-line gaming, and market research industries." Margin="24,0,0,0"/>
        <TextBlock Text="Expertise in requirements capture, analysis, design, implementation, test, deploy, documentation, operations and support phases." Margin="24,0,0,0"/>
        
    </StackPanel>
</UserControl>
