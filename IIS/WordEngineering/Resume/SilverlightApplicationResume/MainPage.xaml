<UserControl x:Class="SilverlightApplicationResume.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    xmlns:jl="clr-namespace:SilverlightApplicationResume"             
    d:DesignHeight="300" d:DesignWidth="400">

    <StackPanel x:Name="LayoutRoot" Background="Beige" Orientation="Vertical">
        <jl:ContactInformation x:Name="contactInformation" />
        <jl:Objective x:Name="objective" />
        <jl:CareerSummary x:Name="careerSummary" />
        <jl:JobDescriptions x:Name="jobDescriptions" />
        <jl:Education x:Name="education" />
    </StackPanel>
</UserControl>
